<article class="card">
    <div class="card-image">
        <figure class="image">
            <img src="<%= get_image @property %>">
        </figure>
    </div>
    <div class="card-content content">
      <%= for {k, v} <- get_data @property do %>
        <div class="message">
          <div class="message-header">
            <p> <%= k %> </p>
          </div>
          <div class="message-body">
            <%= v %>
          </div>
        </div>
      <% end %>
    </div>
</article>
